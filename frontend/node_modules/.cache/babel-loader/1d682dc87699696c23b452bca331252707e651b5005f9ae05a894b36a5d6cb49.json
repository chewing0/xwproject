{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'ModuleTwoView',\n  data() {\n    return {\n      normalLog: null,\n      testLog: null,\n      loading: false,\n      comparisonResult: null\n    };\n  },\n  computed: {\n    canCompare() {\n      return this.normalLog && this.testLog && !this.loading;\n    }\n  },\n  methods: {\n    handleNormalLogUpload(file) {\n      if (this.validateFile(file)) {\n        this.normalLog = file.raw;\n        this.comparisonResult = null;\n      }\n      return false;\n    },\n    handleTestLogUpload(file) {\n      if (this.validateFile(file)) {\n        this.testLog = file.raw;\n        this.comparisonResult = null;\n      }\n      return false;\n    },\n    handleNormalLogRemove() {\n      this.normalLog = null;\n      this.comparisonResult = null;\n    },\n    handleTestLogRemove() {\n      this.testLog = null;\n      this.comparisonResult = null;\n    },\n    validateFile(file) {\n      const isTxt = file.type === 'text/plain';\n      const isLt2M = file.size / 1024 / 1024 < 2;\n      if (!isTxt) {\n        this.$message.error('只能上传 TXT 文件!');\n        return false;\n      }\n      if (!isLt2M) {\n        this.$message.error('文件大小不能超过 2MB!');\n        return false;\n      }\n      return true;\n    },\n    async compareLogs() {\n      if (!this.canCompare) {\n        this.$message.error('请上传正常日志和测试日志文件');\n        return;\n      }\n      this.loading = true;\n      this.comparisonResult = null;\n      try {\n        const formData = new FormData();\n        formData.append('normal_log', this.normalLog);\n        formData.append('test_log', this.testLog);\n        console.log('Sending files:', {\n          normalLog: this.normalLog.name,\n          testLog: this.testLog.name\n        });\n        const response = await axios.post('http://localhost:8000/api/anomaly-detection/', formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        this.comparisonResult = response.data;\n        this.$message.success('日志对比完成');\n      } catch (error) {\n        console.error('日志对比失败:', error);\n        if (error.response) {\n          console.error('Error response:', error.response.data);\n        }\n        this.comparisonResult = {\n          error: error.response?.data?.error || '日志对比失败'\n        };\n        this.$message.error(error.response?.data?.error || '日志对比失败');\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","normalLog","testLog","loading","comparisonResult","computed","canCompare","methods","handleNormalLogUpload","file","validateFile","raw","handleTestLogUpload","handleNormalLogRemove","handleTestLogRemove","isTxt","type","isLt2M","size","$message","error","compareLogs","formData","FormData","append","console","log","response","post","headers","success"],"sources":["D:\\wby\\projectfiles\\xwproject\\frontend\\src\\views\\Module2.vue"],"sourcesContent":["<template>\r\n  <div class=\"module2-container\">\r\n    <el-card class=\"comparison-card\">\r\n      <template #header>\r\n        <div class=\"card-header\">\r\n          <h2>日志对比分析</h2>\r\n        </div>\r\n      </template>\r\n      \r\n      <div class=\"upload-section\">\r\n        <div class=\"upload-group\">\r\n          <h3>正常日志</h3>\r\n          <el-upload\r\n            class=\"upload-demo\"\r\n            action=\"#\"\r\n            :auto-upload=\"false\"\r\n            :on-change=\"handleNormalLogUpload\"\r\n            :on-remove=\"handleNormalLogRemove\"\r\n            :limit=\"1\"\r\n            accept=\".txt\"\r\n          >\r\n            <template #trigger>\r\n              <el-button type=\"primary\">选择文件</el-button>\r\n            </template>\r\n            <template #tip>\r\n              <div class=\"el-upload__tip\">\r\n                请上传正常运行的日志文件\r\n              </div>\r\n            </template>\r\n          </el-upload>\r\n          <div v-if=\"normalLog\" class=\"file-info\">\r\n            已选择: {{ normalLog.name }}\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"upload-group\">\r\n          <h3>测试日志</h3>\r\n          <el-upload\r\n            class=\"upload-demo\"\r\n            action=\"#\"\r\n            :auto-upload=\"false\"\r\n            :on-change=\"handleTestLogUpload\"\r\n            :on-remove=\"handleTestLogRemove\"\r\n            :limit=\"1\"\r\n            accept=\".txt\"\r\n          >\r\n            <template #trigger>\r\n              <el-button type=\"primary\">选择文件</el-button>\r\n            </template>\r\n            <template #tip>\r\n              <div class=\"el-upload__tip\">\r\n                请上传需要检测的日志文件\r\n              </div>\r\n            </template>\r\n          </el-upload>\r\n          <div v-if=\"testLog\" class=\"file-info\">\r\n            已选择: {{ testLog.name }}\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"action-section\">\r\n        <el-button \r\n          type=\"primary\" \r\n          :loading=\"loading\"\r\n          :disabled=\"!canCompare\"\r\n          @click=\"compareLogs\"\r\n        >\r\n          开始对比\r\n        </el-button>\r\n      </div>\r\n\r\n      <div v-if=\"comparisonResult\" class=\"result-section\">\r\n        <h3>对比结果</h3>\r\n        <el-alert\r\n          v-if=\"comparisonResult.error\"\r\n          :title=\"comparisonResult.error\"\r\n          type=\"error\"\r\n          :closable=\"false\"\r\n          show-icon\r\n        />\r\n        <div v-else class=\"result-content\">\r\n          <p><strong>异常数量：</strong>{{ comparisonResult.anomaly_count }}</p>\r\n          <div v-if=\"comparisonResult.anomalies && comparisonResult.anomalies.length > 0\">\r\n            <p><strong>异常详情：</strong></p>\r\n            <el-collapse>\r\n              <el-collapse-item \r\n                v-for=\"(anomaly, index) in comparisonResult.anomalies\" \r\n                :key=\"index\"\r\n                :title=\"`异常 ${index + 1}`\"\r\n              >\r\n                <div class=\"anomaly-detail\">\r\n                  <p><strong>异常类型：</strong>{{ anomaly.type }}</p>\r\n                  <p><strong>异常描述：</strong>{{ anomaly.description }}</p>\r\n                  <p><strong>异常位置：</strong>{{ anomaly.location }}</p>\r\n                </div>\r\n              </el-collapse-item>\r\n            </el-collapse>\r\n          </div>\r\n          <div v-else class=\"no-anomalies\">\r\n            <el-alert\r\n              title=\"未检测到异常\"\r\n              type=\"success\"\r\n              :closable=\"false\"\r\n              show-icon\r\n            />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: 'ModuleTwoView',\r\n  data() {\r\n    return {\r\n      normalLog: null,\r\n      testLog: null,\r\n      loading: false,\r\n      comparisonResult: null\r\n    }\r\n  },\r\n  computed: {\r\n    canCompare() {\r\n      return this.normalLog && this.testLog && !this.loading\r\n    }\r\n  },\r\n  methods: {\r\n    handleNormalLogUpload(file) {\r\n      if (this.validateFile(file)) {\r\n        this.normalLog = file.raw\r\n        this.comparisonResult = null\r\n      }\r\n      return false\r\n    },\r\n\r\n    handleTestLogUpload(file) {\r\n      if (this.validateFile(file)) {\r\n        this.testLog = file.raw\r\n        this.comparisonResult = null\r\n      }\r\n      return false\r\n    },\r\n\r\n    handleNormalLogRemove() {\r\n      this.normalLog = null\r\n      this.comparisonResult = null\r\n    },\r\n\r\n    handleTestLogRemove() {\r\n      this.testLog = null\r\n      this.comparisonResult = null\r\n    },\r\n\r\n    validateFile(file) {\r\n      const isTxt = file.type === 'text/plain'\r\n      const isLt2M = file.size / 1024 / 1024 < 2\r\n\r\n      if (!isTxt) {\r\n        this.$message.error('只能上传 TXT 文件!')\r\n        return false\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error('文件大小不能超过 2MB!')\r\n        return false\r\n      }\r\n      return true\r\n    },\r\n\r\n    async compareLogs() {\r\n      if (!this.canCompare) {\r\n        this.$message.error('请上传正常日志和测试日志文件')\r\n        return\r\n      }\r\n\r\n      this.loading = true\r\n      this.comparisonResult = null\r\n\r\n      try {\r\n        const formData = new FormData()\r\n        formData.append('normal_log', this.normalLog)\r\n        formData.append('test_log', this.testLog)\r\n\r\n        console.log('Sending files:', {\r\n          normalLog: this.normalLog.name,\r\n          testLog: this.testLog.name\r\n        })\r\n\r\n        const response = await axios.post('http://localhost:8000/api/anomaly-detection/', formData, {\r\n          headers: {\r\n            'Content-Type': 'multipart/form-data'\r\n          }\r\n        })\r\n\r\n        this.comparisonResult = response.data\r\n        this.$message.success('日志对比完成')\r\n      } catch (error) {\r\n        console.error('日志对比失败:', error)\r\n        if (error.response) {\r\n          console.error('Error response:', error.response.data)\r\n        }\r\n        this.comparisonResult = {\r\n          error: error.response?.data?.error || '日志对比失败'\r\n        }\r\n        this.$message.error(error.response?.data?.error || '日志对比失败')\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.module2-container {\r\n  padding: 20px;\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.comparison-card {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.card-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.card-header h2 {\r\n  margin: 0;\r\n  color: #303133;\r\n}\r\n\r\n.upload-section {\r\n  display: flex;\r\n  gap: 40px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.upload-group {\r\n  flex: 1;\r\n  padding: 20px;\r\n  border: 1px solid #ebeef5;\r\n  border-radius: 4px;\r\n  background-color: #fafafa;\r\n}\r\n\r\n.upload-group h3 {\r\n  margin-top: 0;\r\n  margin-bottom: 15px;\r\n  color: #303133;\r\n}\r\n\r\n.file-info {\r\n  margin-top: 10px;\r\n  color: #606266;\r\n  font-size: 14px;\r\n}\r\n\r\n.action-section {\r\n  text-align: center;\r\n  margin: 20px 0;\r\n}\r\n\r\n.result-section {\r\n  margin-top: 20px;\r\n  padding: 20px;\r\n  background-color: #f5f7fa;\r\n  border-radius: 4px;\r\n}\r\n\r\n.result-section h3 {\r\n  margin-top: 0;\r\n  margin-bottom: 15px;\r\n  color: #303133;\r\n}\r\n\r\n.result-content {\r\n  color: #606266;\r\n}\r\n\r\n.anomaly-detail {\r\n  padding: 10px;\r\n  background-color: #fff;\r\n  border-radius: 4px;\r\n  margin: 5px 0;\r\n}\r\n\r\n.anomaly-detail p {\r\n  margin: 5px 0;\r\n}\r\n\r\n.no-anomalies {\r\n  margin-top: 15px;\r\n}\r\n\r\n.el-upload__tip {\r\n  color: #909399;\r\n  font-size: 12px;\r\n  margin-top: 5px;\r\n}\r\n\r\n:deep(.el-upload-list) {\r\n  margin-top: 10px;\r\n}\r\n\r\n:deep(.el-collapse-item__header) {\r\n  font-size: 14px;\r\n  color: #303133;\r\n}\r\n\r\n:deep(.el-collapse-item__content) {\r\n  padding: 10px;\r\n}\r\n</style> "],"mappings":"AAkHA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI;MACfC,OAAO,EAAE,IAAI;MACbC,OAAO,EAAE,KAAK;MACdC,gBAAgB,EAAE;IACpB;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACL,SAAQ,IAAK,IAAI,CAACC,OAAM,IAAK,CAAC,IAAI,CAACC,OAAM;IACvD;EACF,CAAC;EACDI,OAAO,EAAE;IACPC,qBAAqBA,CAACC,IAAI,EAAE;MAC1B,IAAI,IAAI,CAACC,YAAY,CAACD,IAAI,CAAC,EAAE;QAC3B,IAAI,CAACR,SAAQ,GAAIQ,IAAI,CAACE,GAAE;QACxB,IAAI,CAACP,gBAAe,GAAI,IAAG;MAC7B;MACA,OAAO,KAAI;IACb,CAAC;IAEDQ,mBAAmBA,CAACH,IAAI,EAAE;MACxB,IAAI,IAAI,CAACC,YAAY,CAACD,IAAI,CAAC,EAAE;QAC3B,IAAI,CAACP,OAAM,GAAIO,IAAI,CAACE,GAAE;QACtB,IAAI,CAACP,gBAAe,GAAI,IAAG;MAC7B;MACA,OAAO,KAAI;IACb,CAAC;IAEDS,qBAAqBA,CAAA,EAAG;MACtB,IAAI,CAACZ,SAAQ,GAAI,IAAG;MACpB,IAAI,CAACG,gBAAe,GAAI,IAAG;IAC7B,CAAC;IAEDU,mBAAmBA,CAAA,EAAG;MACpB,IAAI,CAACZ,OAAM,GAAI,IAAG;